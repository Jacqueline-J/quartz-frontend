{
  "pipeline": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "@openclimatefix/nowcasting-app#build": {
      "dependsOn": [
        "^build",
        "$AUTH0_SECRET",
        "$AUTH0_BASE_URL",
        "$AUTH0_ISSUER_BASE_URL",
        "$AUTH0_CLIENT_ID",
        "$AUTH0_CLIENT_SECRET",
        "$SENTRY_AUTH_TOKEN"
      ],
      "outputs": [
        ".next/**"
      ]
    },
    "test": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  }
}
